<div class="row" style="margin-left: -10px;">
    <div class="hidden-xs hidden-if hidden-sm col-md-12 col-lg-12">
        <div class="col-md-9">
            <div class="btn-group text-center bottom-buffer" role="group" aria-label="...">

                <a role="button" class="btn btn-default @(ViewData.Eval("page")=="home"?"disabled":"")"
                   onclick="RedirectAction(@ViewBag.ReturnUrl, 'Index')" data-toggle="tooltip"
                   data-placement="bottom" style="padding: 10px 20px;">
                    <span class="icon-home " aria-hidden="true"></span>&nbsp;Inicio
                </a>
                <a role="button" class="btn btn-default @(ViewData.Eval("page")=="about"?"disabled":"")"
                   onclick="RedirectAction(@ViewBag.ReturnUrl, 'About')" data-toggle="tooltip"
                   data-placement="bottom" style="padding: 10px 20px;">
                    <span class="glyphicon glyphicon-info-sign" aria-hidden="true"></span>&nbsp;Acerca del trámite
                </a>
                <a role="button" class="btn btn-default @(ViewData.Eval("page")=="regitroSolicitud"?"disabled":"")" onclick="RedirectAction(@ViewBag.ReturnUrl, 'Solicitud')" data-toggle="tooltip"
                   data-placement="bottom" role="button" style="padding: 10px 20px;">
                    <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>&nbsp;Registrar solicitud
                </a>
                <a role="button" class="btn btn-default @(ViewData.Eval("page")=="verSolicitudes"?"disabled":"")" onclick="RedirectAction(@ViewBag.ReturnUrl, 'Historial')" data-toggle="tooltip"
                   data-placement="bottom" role="button" style="padding: 10px 20px;">
                    <span class="glyphicon glyphicon-calendar" aria-hidden="true"></span>&nbsp;Ver mis solicitudes
                </a>
            </div>
        </div>
    </div>
</div> 
