@model IEnumerable<ISSSTE.Tramites2016.Hipotecas.Model.Model.RequestAdministrator>

@{
    ViewBag.Title = "Trámite de solicitud de cancelación de hipotecas vía administrativa del ISSSTE";
    Layout = "~/Views/Shared/_LayoutEntitle.cshtml";
}
<script type="text/javascript">
    function RedirectAction(id, page, requestId) {
        window.location.href = "./RedirectPage?NoIssste=" + id + "&Page=" + page + "&RequestId=" + requestId;
    }
</script>
<div class="container" data-ng-controller="requestController as vm">
    <div class="container" style="margin-top: -2px;">
        <div class="row">
            <div class="col-xs-12 col-sm-8" style="margin-top: -6px;">
                <ol class="breadcrumb  col-md-8" id="nave">
                    <li><a href="http://www.gob.mx"><i class="icon icon-home"></i></a><i id="navinicio" class="active"><a href="http://www.gob.mx"></a></i></li>
                    <li id="navinicio"><a href="~/Entitle/index?noissste=@ViewBag.NoIssste#/Home">Inicio</a></li>
                    <li class="active">Mis solicitudes</li>
                </ol>
            </div>
            <div class="col-xs-12 col-sm-4" style="margin-top: -12px;">
                @Html.Partial("_InfoUsuario", new { UserName = ViewBag.Name })
            </div>

            <div class="hidden-xs col-xs-8 ">
                <h2 class="vertical-buffer">@ViewBag.Title</h2>
            </div>

            <div class="row" data-ng-hide="nc.isOverrided">

                @Html.Partial("_EntitleMenu", new { page = "verSolicitudes" })

            </div>
        </div>

        <div>
            <!--Start Wizard Steps-->

            <div class="row">
                <div class="col-xs-12">
                    <h3>Mis solicitudes</h3>
                    <hr class="red">

                </div>
                <div class="col-xs-12">


                    <table class="table  bottom-buffer">
                        <thead>
                            <tr>
                                <th>Folio</th>
                                <th>Fecha</th>
                                <th>Estatus</th>
                                <th></th>
                            </tr>
                        </thead>
                        <tbody>

                            @if (Model != null)
                            {
                                foreach (var item in Model)
                                {
                                    <tr>
                                        <td style="border:none;height:auto">@Html.DisplayFor(modelitem => item.Folio)</td>
                                        <td style="border:none;height:auto">@Html.DisplayFor(modelitem => item.Date)</td>
                                        <td style="border:none;height:auto">@Html.DisplayFor(modelitem => item.Estatus).</td>
                                        <td style="border:none;height:auto">
                                            <div class="btn-group pull-right" role="group">
                                                <table>
                                                    <tr>
                                                        <td>
                                                            <div data-ng-show="@item.StatusId == 200 || @item.StatusId == 202 ||@item.StatusId == 206 || @item.StatusId == 207">
                                                                <a type="button" class="btn btn-primary btn-sm" href="@Url.Action("AgendarCita", "Entitle", new { NoIssste = @ViewBag.NoIssste, RequestId = item.RequestId})" style="cursor: pointer;" onclick="Mostrar()">Agendar cita</a>
                                                            </div>

                                                        </td>
                                                        <td>
                                                            <div align="left" data-ng-show="@item.StatusId == 205">
                                                                <a role="button" class="btn btn-primary btn-sm"
                                                                   href="@Url.Action("Documentacion", "Entitle", new { NoIssste = @ViewBag.NoIssste, RequestId = item.RequestId, Sts = item.StatusId })">
                                                                    Subir documentación
                                                                </a>
                                                            </div>
                                                        </td>
                                                        <td>
                                                            <div>
                                                                <a type="button" class="btn btn-primary btn-sm" onclick="RedirectAction(@(Convert.ToChar(34) + ViewBag.NoIssste + "|" + item.Folio + "|" + item.RequestId + Convert.ToChar(34)), 'SolicitudCompleta','@item.RequestId' ); uid_call('Hipotecas.Historial.VerSolicitudCompleta', 'clickin' ); Mostrar()" style="cursor: pointer;">Ver detalle</a>
                                                                @*<ul class="dropdown-menu" role="menu" aria-labelledby="btnGroupVerticalDrop1">
                                                                        <li ><a  href="@Url.Action("AgendarCita", "Entitle", new { NoIssste = @ViewBag.NoIssste, RequestId = item.RequestId})" onclik="uid_call('Hipotecas.Historial.Agendar', 'clickin' );" style="cursor: pointer;">Agendar cita</a></li>
                                                                        <li><a   onclick="RedirectAction(@(Convert.ToChar(34) + ViewBag.NoIssste + "|" + item.Folio + Convert.ToChar(34)), 'SolicitudCompleta','@item.RequestId' ); uid_call('Hipotecas.Historial.VerSolicitudCompleta', 'clickin' ); " style="cursor: pointer;">Ver Solicitud Completa</a></li>
                                                                    </ul>*@
                                                            </div>                                                            
                                                        </td>
                                                    </tr>
                                                </table>
                                            </div>
                                        </td>
                                    </tr>
                                }
                            }

                        </tbody>
                    </table>

                </div>


            </div>
            @section Scripts{

                @Scripts.Render("~/bundles/entitle/app")
            }

            <!-- End Trámite -->
            <!-- End tramite pensiones -->
        </div><!-- container -->
    </div>

</div>

<script type="text/javascript">
    $(window).load(function () { Oculta(); });
</script>