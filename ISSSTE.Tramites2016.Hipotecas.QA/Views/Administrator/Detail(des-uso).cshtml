@model ISSSTE.Tramites2016.Hipotecas.Model.Model.Entitle
@{
    Layout = "~/Views/Shared/_LayoutAdministrator.cshtml";
}

<script type="text/javascript">
    function RedirectAction(Id, Page, NoIssste) {
        window.location.href = "./RedirectPage?NoIssste=" + NoIssste + "&Page=" + Page + "&Request=" + Id;
    }
</script>
<div class="row" data-ng-controller="navigationController as nc">
    <!--/ START LEFT MENU -->
    <div id="side-menu" class="side-menu collapse">
        <nav class="navbar navbar-default" role="navigation">
            <!-- Main Menu -->
            <div class="side-menu-container">
                <ul class="nav navbar-nav">
                    <li data-ng-show="nc.canUserAccessSearch()" data-ng-class="{ 'active': nc.isSearchSelected}">
                        <a href="/Administrator/ConsultaSolicitudes">Consulta de Solicitudes</a>
                    </li>
                    <li data-ng-show="nc.canUserAccessQuotasManager()" data-ng-class="{ 'active': nc.isQuotasManagerSelected}">
                        <a href="/Administrator/AdministracionCitas">Administración de citas</a>
                    </li>
                    <li data-ng-show="nc.canUserAccessApprovalQuotasDelegation()" data-ng-class="{ 'active': nc.isApprovalQuotasDelegationSelected}">
                        <a href="/Administrator/ConsultaCitas">Consulta de Citas</a>
                    </li>


                </ul>
            </div><!-- /.navbar-collapse -->
        </nav>
    </div> <!-- /.side-menu -->
    <!--/ END LEFT MENU -->

    <div class="container-fluid">
        <div class="side-body ng-view" autoscroll="true">

            <div class="row">
                <div>
                    <ul class="nav nav-tabs" role="tablist">
                        <li role="presentation" class="active">
                            <a href="/Administrator/ConsultaSolicitudes" >Derechohabiente</a>
                        </li>
                        <li role="presentation">
                            <a onclick=" javascript:RedirectAction('@ViewBag.ReturnUrl', 'Documentos');" style="cursor: pointer" role="tab" data-toggle="tab">Documentos</a>
                        </li>
                        <li role="presentation">
                            <a onclick=" javascript:RedirectAction('@ViewBag.ReturnUrl', 'RequestView', @Model.NoISSSTE);" style="cursor: pointer" role="tab" data-toggle="tab">Datos de la solicitud</a>
                        </li>
                        <li role="presentation">
                            <a onclick=" javascript:RedirectAction('@ViewBag.ReturnUrl');" style="cursor: pointer" role="tab" data-toggle="tab">Resultado de la revisión</a>
                        </li>
                        <li role="presentation">
                            <a href="/Administrator/Seguimiento"  role="tab" data-toggle="tab">Seguimiento</a>
                        </li>
                    </ul>
                </div>

                <div role="tabpanel" class="tab-pane active" id="entitle">

                    <div class="form-group col-xs-6">
                        <h3>Información generales</h3>
                    </div>

                    <div class="col-xs-3">
                        <div class="pull-right margintop-20">
                            <a href="/Administrator/ConsultaSolicitudes" class="btn btn-default" type="button">
                                Volver a consulta de solicitudes
                            </a>
                        </div>
                    </div>
                    @{
                    <div class="col-xs-12">
                        <div class="form-group col-md-4" data-ng-show="vm.requestInfo.Entitle.NoISSSTE">
                            <label class="control-label">Número de ISSSTE:</label>
                            <input class="form-control ng-valid" type="text" value="@Html.DisplayFor(modelItem => Model.NoISSSTE)" disabled="">
                        </div>
                        <div class="form-group col-md-6">
                            <label class="control-label">Clave Única de Registro de Población (CURP):</label>
                            <input class="form-control ng-valid" type="text"value="@Html.DisplayFor(modelItem => Model.CURP)" disabled="">
                        </div>
                        <div class="form-group col-md-6">
                            <label class="control-label">Registro Federal del Contribuyente (RFC):</label>
                            <input class="form-control ng-valid" type="text" value="@Html.DisplayFor(modelItem => Model.RFC)" disabled="">
                        </div>
                        <div class="form-group col-md-4">
                            <label class="control-label">Nombre(s):</label>

                            <input class="form-control ng-valid" type="text" value="@Html.DisplayFor(modelItem => Model.Name)" style="text-transform:uppercase" disabled="">

                        </div>
                        <div class="form-group col-md-4">
                            <label class="control-label">Primer apellido:</label>
                            <input class="form-control ng-valid" type="text" value="@Html.DisplayFor(modelItem => Model.PaternalLastName)" style="text-transform:uppercase" disabled="">
                        </div>
                        <div class="form-group col-md-4">
                            <label class="control-label">Segundo apellido:</label>
                            <input class="form-control ng-valid" type="text" value="@Html.DisplayFor(modelItem => Model.MaternalLastName)" style="text-transform:uppercase" disabled="">
                        </div>

                        <div class="form-group col-md-4">
                            <label class="control-label">Sexo:</label>
                            <input class="form-control ng-valid" type="text" value="@Html.DisplayFor(modelItem => Model.Gender)" disabled="">
                        </div>

                        <div class="form-group col-md-4">
                            <label class="control-label">Fecha de nacimiento:</label>
                            <input class="form-control" type="text" value="@Html.DisplayFor(modelItem => Model.Birthdate)" disabled="">
                        </div>
                        <div class="form-group col-md-4">
                            <label class="control-label">Lugar de nacimiento:</label>
                            <input class="form-control ng-valid" type="text" value="@Html.DisplayFor(modelItem => Model.BirthPlace)" disabled="">
                        </div>
                        <div class="form-group col-md-4">
                            <label class="control-label">Estado civil:</label>
                            <input class="form-control ng-valid" type="text" value="@Html.DisplayFor(modelItem => Model.MaritalStatus)" disabled="">
                        </div>
                        <div class="form-group col-xs-12">
                            <h3>Dirección</h3>
                        </div>
                        <div class="form-group col-md-2">
                            <label class="control-label">Código postal:</label>
                            <input class="form-control ng-valid" type="text" value="@Html.DisplayFor(modelItem => Model.ZipCode)" disabled="">
                        </div>
                        <div class="form-group col-md-6">
                            <label class="control-label">Nombre de vialidad:</label>
                            <input class="form-control ng-valid" type="text" value="@Html.DisplayFor(modelItem => Model.Street)" disabled="">
                        </div>
                        <div class="form-group col-md-2">
                            <label class="control-label">Número exterior:</label>
                            <input class="form-control ng-valid" type="text" value="@Html.DisplayFor(modelItem => Model.NumExt)" disabled="">
                        </div>
                        <div class="form-group col-md-2">
                            <label class="control-label">Número interior:</label>
                            <input class="form-control ng-valid" type="text" value="@Html.DisplayFor(modelItem => Model.NumInt)" disabled="">
                        </div>
                        <div class="form-group col-md-6">
                            <label class="control-label">Nombre del asentamiento humano o colonia: </label>
                            <input class="form-control ng-valid" type="text" value="@Html.DisplayFor(modelItem => Model.Colony)" disabled="">
                        </div>

                        <div class="form-group col-md-6">
                            <label class="control-label">Nombre del Municipio o Delegación:</label>
                            <input class="form-control ng-valid" type="text" value="@Html.DisplayFor(modelItem => Model.City)" disabled="">
                        </div>
                        <h3 class="col-xs-12">Datos de contacto</h3>


                        <div class="form-group col-md-6">
                            <label class="control-label">Teléfono:</label>
                            <input class="form-control ng-valid" type="text" value="@Html.DisplayFor(modelItem => Model.Telephone)" disabled="">
                        </div>
                        <div class="form-group col-md-6">
                            <label class="control-label">Correo electrónico:</label>
                            <input class="form-control ng-valid" type="text" value="@Html.DisplayFor(modelItem => Model.Email)" disabled="">
                        </div>
                    </div>
                    }

                    
                    <!-- End Tab -->

                </div>
                <div role="tabpanel" class="tab-pane" id="docs">

                </div>
            </div>
        </div>

    </div>



</div>



