@model ISSSTE.Tramites2016.Hipotecas.Model.Model.Request
@{
    Layout = "~/Views/Shared/_LayoutAdministrator.cshtml";
}

<script type="text/javascript">
    function RedirectAction(Id, Page, NoIssste, Observations) {
        window.location.href = "./RedirectPage?NoIssste=" + NoIssste + "&Page=" + Page + "&Request=" + Id + "&Obeservations" + Observations;
    }
</script>
<div class="row">

    @Html.Partial("_AdministratorMenu")

    <div class="container-fluid">
        <div class="side-body " autoscroll="true">

            <div class="row">

                @Html.Partial("DetailEntitleNavTabs")

                <div role="tabpanel" class="tab-pane active" id="entitle">
                    <div class="col-xs-12">
                        <div class="col-xs-8">
                            <h2>Información general</h2>
                            <hr class="red">
                        </div>

                        <div class="col-xs-4 margintop-30">
                            <div class="pull-right">
                                <a href="@System.Configuration.ConfigurationManager.AppSettings["Sitio"]/Administrator/Index/#search" class="btn btn-default" type="button">
                                    Volver a consulta de solicitudes
                                </a>
                            </div>
                        </div>
                    </div>

                    @{
                        <div class="col-xs-12 margintop-40">
                            <div class="form-group col-md-4" data-ng-show="vm.requestInfo.Entitle.NoISSSTE">
                                <label class="control-label">Folio:</label>
                                <input class="form-control ng-valid" type="text" value="@Html.DisplayFor(modelItem => Model.Folio)" disabled="">
                            </div>
                            <div class="form-group col-md-4">
                                <label class="control-label">Fecha de solicitud:</label>
                                <input class="form-control ng-valid" type="text" value="@Convert.ToString(string.Format("{0:dd/MM/yyyy}", Model.Date))" disabled="">
                            </div>
                            <div class="form-group col-md-4">
                                <label class="control-label">Tipo de crédito:</label>
                                @if (Model.IsConjugalCredit)
                                {
                                    <input class="form-control ng-valid" type="text" value="Conyugal" disabled="">
                                }
                                else
                                {
                                    <input class="form-control ng-valid" type="text" value="Individual" disabled="">
                                }

                            </div>

                            <div class="form-group col-md-12">
                                <label class="control-label">Datos de Ubicación de Escritura:</label>
                                <input class="form-control ng-valid" type="text" value="@Html.DisplayFor(modelItem => Model.WritingProperty)" disabled="">
                            </div>

                        </div>
                    }


                    <!-- End Tab -->

                </div>
            </div>
        </div>

    </div>



</div>
<br />
<br />
