@model List<ISSSTE.Tramites2016.Hipotecas.Model.Model.DocumentType>
@{
    ViewBag.Title = "Trámite de solicitud de cancelación de hipotecas vía administrativa del ISSSTE";
    Layout = "~/Views/Shared/_LayoutEntitle.cshtml";
}
<script>
    function RedirectAction(Id, Page) {
        window.location.href = "./RedirectPage?NoIssste=" + Id + "&Page=" + Page;
    }
</script>

<div class="container" style="margin-top: -2px;" ng-controller="requestController as vm">
    <div class="row">
        <div class="col-xs-12 col-sm-8">
            <ol class="breadcrumb  col-md-8" id="nave">
                <li><a href="http://www.gob.mx"><i class="icon icon-home"></i></a><i id="navinicio" class="active"><a href="http://www.gob.mx"></a></i></li>
                <li id="navinicio"><a href="~/Entitle/index?noissste=@ViewBag.NoIssste#/Home">Inicio</a></li>
                <li class="active">Requisitos del trámite</li>
            </ol>
        </div>
        <div class="col-xs-12 col-sm-4">
            @Html.Partial("_InfoUsuario", new { UserName = ViewBag.Name })
        </div>

        <div class="hidden-xs col-xs-8 ">
            <h2 class="vertical-buffer">@ViewBag.Title</h2>

        </div>
        <div class="row">
            <!--oculta el menu -->
            @*<div class="clearfix">

                </div>*@

            @Html.Partial("_EntitleMenu", new { page = "regitroSolicitud" })

        </div>
    </div>

    <div>


        <!--Start Wizard Steps-->



        <ul class="wizard-steps" style="margin-left: 0px">
            <li class="completed" id="A" style="padding: 0 14px 0 50px;">
                <h5>Paso 1</h5>
                <span>Requisitos del trámite</span>
            </li>
            <li id="B">
                <h5>Paso 2</h5>
                <span>Información de inmueble</span>
            </li>
            <li id="D" style="width: 280px">
                <h5>Paso 3</h5>
                <span>Cargar documentación</span>
            </li>
            <li>
                <i class="glyphicon glyphicon-ok-circle"></i>
            </li>
        </ul>

        @if (ViewBag.Message == "Error")
        {
            <div class="alert alert-warning">
                <div>@ViewBag.Message1</div>
            </div>
        }


        <div class="row">
            <div class="col-md-12">
                <p>Estos son los pasos que tendrás que seguir para completar tu solicitud.</p>
            </div>
        </div>

        <!--End Wizard Steps-->
        <!-- Start Trámite -->

        <div id="requisitos" class="row">

            <div class="col-xs-12">
                <h3>Información general sobre este trámite</h3>
                <hr class="red" />
                

                @if (@ViewBag.requestActive)
                {
                    <div class="alert alert-info" >
                        <strong>¡Atención!</strong> Ya cuentas con un trámite en proceso.
                    </div>

                }

                <div class="alert alert-info">
                    En caso de dudas, favor de comunicarse al teléfono: 01(55) 5322-0497
                </div>
   
                <p>Apartado de requisitos para continuar con el proceso de registro de inmueble.</p>
                <br />
                <ul class="nav nav-tabs">
                    <li class="active">
                        <a data-toggle="tab">Requisitos para realizar el trámite</a>
                    </li>
                </ul>
                <div class="tab-content marginbot-20">
                    <div class="tab-pane active " id="tab-01">
                        <div class="alert alert-info">
                            Te pedimos cubrir los siguientes requisitos antes de comenzar el trámite.
                        </div>
                        @{
                                        var title1 = 0;
                                        var title2 = 0;
                                        var title3 = 0;
                                        var title4 = 0;
                                        var title5 = 0;

                                        foreach (var item in Model)
                                        {
                                            if (item.BeneficiarieType == 1 && title1 == 0)
                                            {
                                    <h5>Para Derehohabiente:</h5>
                                    title1 = 1;
                                            }
                                            if (item.BeneficiarieType == 2 && title2 == 0)
                                            {
                                    <div class="alert alert-info">
                                        Este documento se tramita directamente en las oficinas centrales de ISSSTE en la Jefatura de Cartera y Cobranza, Dirección de Prestaciones Económicas Sociales
                                        y Culturales del ISSSTE.
                                        Domicilio: Jesús García No. 140, piso 7 Ala "B", Col. Buenavista, Del. Cuahutémoc, México, D.F.
                                        Teléfono: 51409617, ext. 15266.
                                    </div>
                                    <h5>Adicionalmente tendrás que presentar la documentación correspondiente  en los casos siguientes:</h5>
                                    <h5>1. Para apoderado o mandatario del derechohabiente:</h5>
                                    title2 = 1;
                                }
                                if (item.BeneficiarieType == 3 && title3 == 0)
                                {
                                    <div class="alert alert-info">
                                        En caso de no ser el titular del trámite deberá acreditar el interés jurídico con la siguiente documentación.
                                    </div>
                                    <h5>2. En caso de no ser titular:</h5>
                                    title3 = 1;
                            }
                                if (item.BeneficiarieType == 4 && title4 == 0)
                                {
                                    <h5>3. Si es Crédito Conyugal:</h5>
                                    title3 = 1;
                                }
                                if (item.BeneficiarieType == 5 && title5 == 0)
                                {
                                    <div class="alert alert-info">
                                        En caso de ser el dueño del inmueble diferente al titular de la hipoteca deberá acreditar el interés jurídico con la siguiente documentación.
                                    </div>
                                    <h5>4. En caso de ser el dueño diferente al de la hipoteca:</h5>
                                    title5 = 1;
                                }
                                <ul>
                                    <li>
                                        <span>@Html.DisplayFor(modelItem => item.Description)</span>

                                    </li>
                                </ul>
                            }
                        }
                    </div>
                </div>

                <!-- Botones -->


            </div> <!-- col -->


        </div> <!-- row -->

        <div class="col-if-12">
            <div class="clearfix " style="margin-bottom: 50px; margin-top: 20px;">
                <div class="pull-right">

                    <a id="printButton" class="btn btn-default" type="button" onclick=" javascript:imprSelec('requisitos') ">
                        <span class="icon-printing" aria-hidden="true"></span>
                        Imprimir
                    </a>
                    @if (@ViewBag.requestActive )
                    {
                         <input class="btn btn-primary" onclick="RedirectAction(@ViewBag.NoIssste, 'Registro');Mostrar()" disabled type="button" value="Siguiente paso"/>
                    }
                    else
                    {
                         <input class="btn btn-primary" onclick="RedirectAction(@ViewBag.NoIssste, 'Registro');Mostrar()"  type="button" value="Siguiente paso"/>
                    }
                   

                        
                </div>
            </div>
            <!-- End Trámite -->
            <!-- End tramite pensiones -->
            @section Scripts{

                @Scripts.Render("~/bundles/entitle/app")
            }
        </div><!-- container -->
    </div>
</div>
<script type="text/javascript">

    function imprSelec(requisitos)
    { var ficha = document.getElementById(requisitos); var ventimp = window.open(' ', 'popimpr'); ventimp.document.write(ficha.innerHTML); ventimp.document.close(); ventimp.print(); ventimp.close(); }

    $(window).load(function () { Oculta(); });
</script>